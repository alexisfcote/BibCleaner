isbn = {9781457717598},
keywords = {-cable driven,6 dimensional force,although cable-driven hap tic,cables (mechanical),device has been studied,feedback,for,haptic interfaces,large-scale,or measure the back,several years,the method to analyze,virtual reality},
pages = {121--125},
